{
    "name": "jstormes/ldap",
    "description": "ldap connector",
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.0",
        "ext-ldap": "*"
    },
    "require-dev": {
        "eloquent/liberator": "^2.0",
        "eloquent/phony-phpunit": "^1.0|^3.0",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "phpunit/phpunit": "^5.7|^6.0",
        "squizlabs/php_codesniffer": "^2.3|^3.0"
    },
    "keywords": [
        "ldap",
        "ad",
        "authorization",
        "authentication",
        "idp",
        "identity",
        "sso",
        "single sign on"
    ],
    "authors": [
        {
            "name": "James Stormes",
            "email": "jstormes@stormes.net",
            "role": "Developer"
        }
    ],
    "autoload": {
            "psr-4": {
                "JStormes\\Ldap\\": "src/"
            }
        },
        "autoload-dev": {
            "psr-4": {
                "JStormes\\Ldap\\Test\\": "test/src/"
        }
    }
}
